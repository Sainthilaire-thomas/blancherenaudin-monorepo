{
    "name":  "@repo/database",
    "version":  "0.0.0",
    "private":  true,
    "main":  "./src/index.ts",
    "types":  "./src/index.ts",
    "exports":  {
                    ".":  {
                              "types":  "./src/index.ts",
                              "default":  "./src/index.ts"
                          },
                    "./server":  {
                                     "types":  "./src/server.ts",
                                     "default":  "./src/server.ts"
                                 },
                    "./client-browser":  {
                                             "types":  "./src/client-browser.ts",
                                             "default":  "./src/client-browser.ts"
                                         },
                    "./client-server":  {
                                            "types":  "./src/client-server.ts",
                                            "default":  "./src/client-server.ts"
                                        },
                    "./types":  {
                                    "types":  "./src/types.ts",
                                    "default":  "./src/types.ts"
                                }
                },
    "scripts":  {
                    "type-check":  "tsc --noEmit",
                    "build":  "tsc --build",
                    "generate:types":  "node scripts/db-types.mjs"
                },
    "dependencies":  {
                         "@supabase/ssr":  "^0.5.2",
                         "@supabase/supabase-js":  "^2.47.10",
                         "stripe":  "^14.25.0"
                     },
    "devDependencies":  {
                            "@types/node":  "^22.10.2",
                            "next":  "^15.0.0",
                            "typescript":  "^5.9.3"
                        },
    "peerDependencies":  {
                             "next":  "^15.0.0"
                         }
}