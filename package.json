{
  "name": "blancherenaudin-monorepo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "turbo run dev",
    "dev:storefront": "turbo run dev --filter=storefront",
    "dev:admin": "turbo run dev --filter=admin",
    "build": "turbo run build",
    "build:storefront": "turbo run build --filter=storefront",
    "build:admin": "turbo run build --filter=admin",
    "type-check": "turbo run type-check",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "test:e2e": "playwright test",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:publish": "changeset publish",
    "doc": "powershell -ExecutionPolicy Bypass -File scripts/generate-packages-doc.ps1",
    "generate-tree": "node scripts/generate-tree.js"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@playwright/test": "^1.40.0",
    "@types/react": "18.3.26",
    "@types/react-dom": "18.3.7",
    "prettier": "^3.1.0",
    "turbo": "^2.5.8",
    "typescript": "^5.3.0"
  },
  "packageManager": "pnpm@8.15.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "18.3.26",
      "@types/react-dom": "18.3.7",
      "react": "18.3.1",
      "react-dom": "18.3.1",
      "react-compiler-runtime": "npm:react@18.3.1"
    }
  }
}
